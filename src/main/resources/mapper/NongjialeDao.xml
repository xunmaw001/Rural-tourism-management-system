<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.NongjialeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.NongjialeEntity" id="nongjialeMap">
        <result property="nongjialemingcheng" column="nongjialemingcheng"/>
        <result property="leibie" column="leibie"/>
        <result property="tupian" column="tupian"/>
        <result property="dizhi" column="dizhi"/>
        <result property="jingyingfanwei" column="jingyingfanwei"/>
        <result property="zixundianhua" column="zixundianhua"/>
        <result property="tesejieshao" column="tesejieshao"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.NongjialeVO" >
		SELECT * FROM nongjiale  nongjiale         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.NongjialeVO" >
		SELECT  nongjiale.* FROM nongjiale  nongjiale
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.NongjialeView" >

		SELECT  nongjiale.* FROM nongjiale  nongjiale 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.NongjialeView" >
		SELECT * FROM nongjiale  nongjiale <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	
</mapper>